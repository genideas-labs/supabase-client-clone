# PostgreSQL 연결 설정
# 방법 1: 개별 매개변수 사용
DB_USER=testuser
DB_HOST=localhost
DB_NAME=testdb
DB_PASS=testpassword
DB_PORT=5432
DB_SSL=false

# 방법 2: 연결 문자열(URI) 사용 (위의 개별 매개변수 대신 사용 가능)
# DB_CONNECTION=postgresql://testuser:testpassword@localhost:5432/testdb

